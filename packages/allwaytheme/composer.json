{
	"name": "aimeos-themes/allwaytheme",
	"description": "Aimeos allwaytheme theme",
	"keywords": ["aimeos", "extension"],
	"type": "aimeos-extension",
	"license": "LGPL-3.0",
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": "^8.0.11",
		"aimeos/aimeos-laravel": "^2023.04"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\": "src"
		},
		"classmap": [
			"src",
			"ExtensionServiceProvider.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Aimeos\\": "tests"
		},
		"classmap": [
			"tests"
		]
	},
    "extra": {
        "laravel": {
            "providers": [
                "Aimeos\\allwaytheme\\ExtensionServiceProvider"
            ]
        }
    }
}